#include<stdio.h>    
int main()
{
	printf("name : CH.Yashwanth\nregno : 192210563");
    int r1,r2,c1,c2;
    printf("\nEnter number of rows for First Matrix:\n");    
    scanf("%d",&r1);    
    printf("\nEnter number of columns for First Matrix:\n");     
    scanf("%d",&c1); 
    printf("\nEnter number of rows for Second Matrix:\n");    
    scanf("%d",&r2);    
    printf("\nEnter number of columns for Second Matrix:\n");     
    scanf("%d",&c2);
    if(c1!=r2)
    {
        printf("\nMatrices Can't be multiplied together");
    }
    else
    {
        int m1[r1][c1],m2[r2][c2];
        printf("\nEnter first matrix elements \n");    
        for(int i=0;i<r1;i++)    
        {    
            for(int j=0;j<c1;j++)    
            {    
                scanf("%d",&m1[i][j]);    
            }    
        }    
        printf("\nEnter Second matrix elements\n");    
        for(int i=0;i<r2;i++)    
        {    
            for(int j=0;j<c2;j++)    
            {    
                scanf("%d",&m2[i][j]);    
            }    
        }    
        int mul[r1][c2];
        for(int i=0;i<r1;i++)    
        {    
            for(int j=0;j<c2;j++)    
            {    
                mul[i][j]=0; 
 
                
                for(int k=0;k<c1;k++)    
                {    
                    mul[i][j]+=m1[i][k]*m2[k][j];    
                } 
            }    
        }    
        printf("\nMultiplied matrix\n");     
        for(int i=0;i<r1;i++)    
        {    
            for(int j=0;j<c2;j++)    
            {    
                printf("\n%d\t",mul[i][j]);    
            }    
            printf("\n");    
        } 
    }
    return 0;  
}
